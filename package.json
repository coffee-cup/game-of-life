{
  "name": "game-of-life",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "Jake Runzer <jakerunzer@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve":
      "cross-env NODE_ENV=development webpack-dev-server --mode=development --hot --inline --progress",
    "build": "yarn run build-release && cross-env NODE_ENV=production webpack --mode=production -p --progress",
    "build-debug": "yarn run compile-debug && yarn run wasm-debug",
    "build-release": "yarn run compile-release && yarn run wasm-release",
    "compile-debug": "cargo +nightly build --target wasm32-unknown-unknown",
    "compile-release":
      "cargo +nightly build --release --target wasm32-unknown-unknown",
    "wasm-debug":
      "wasm-bindgen target/wasm32-unknown-unknown/debug/game_of_life.wasm --out-dir .",
    "wasm-release":
      "wasm-bindgen target/wasm32-unknown-unknown/release/game_of_life.wasm --out-dir .",
    "heroku-postbuild": "yarn run ",
    "test": "echo \"Not implemented\""
  },
  "engines": {
    "node": ">= 9.11.1",
    "npm" : ">= 5.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "normalize-css": "^2.3.1",
    "tachyons": "^4.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "prettier": "^1.12.1",
    "rust-native-wasm-loader": "^0.7.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.3"
  }
}
